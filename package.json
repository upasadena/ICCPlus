{
  "name": "icc-plus",
  "version": "1.4.3",
  "description": "A modified version of the Interactive CYOA Creator by wahaha303.",
  "main": "main.js",
  "build": {
    "appId": "com.plus.icc",
    "win": {
      "target": ["nsis", "squirrel", "portable", "zip"],
      "icon": "./assets/icon-384x384.png"
    },
    "linux": {
      "target": ["AppImage", "deb", "pacman"],
      "icon": "./assets/icon-384x384.png"
    },
    "mac": {
      "category": "public.app-category.games",
      "icon": "./assets/icon-384x384.png"
    }
  },
  "scripts": {
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "env-cmd electron-forge publish"
  },
  "author": "wahaha303 <john.doe@email.com>",
  "email": "john.doe@email.com",
  "maintainers": ["wahaha303 <john.doe@email.com>"],
  "license": "UNLICENSED",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-flatpak": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@pengx17/electron-forge-maker-appimage": "https://github.com/toeverything/electron-forge-maker-appimage.git",
    "electron": "23.1.3",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "env-cmd": "^10.1.0",
    "update-electron-app": "^3.0.0"
  }
}
